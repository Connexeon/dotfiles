# neofetch
alias neofetch2="neofetch \
  --config off \
  --block_range 1 8 \
  --bold off \
  --uptime_shorthand on \
  --gtk_shorthand on \
  --colors 4 1 8 8 8 7 \
"
# Centminmod
alias cmdir='pushd /usr/local/src/centminmod'

# Postfix mail log
alias postfixlog='pflogsumm -d today --verbose_msg_detail /var/log/maillog'
